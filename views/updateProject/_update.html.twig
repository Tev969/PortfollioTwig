<div class="container2">
	<h2>Modifier un projet</h2>
	<form action="/updateproject/{{project.id}}" method="POST">
		<div class="form-group2">
			<label for="titre">Titre du projet :</label>
			<input type="text" id="titre2" name="title" required>
		</div>
		<div class="form-group2">
			<label for="description">Description :</label>
			<textarea id="description2" name="description" rows="4" required></textarea>
		</div>
		<div class="form-group2">
			<label for="technologies">Technologies :</label>
			<input type="text" id="technologies2" name="techno" required>
		</div>
		<div class="form-group2">
			<label for="github">Github Link :</label>
			<input type="text" id="github2" name="github" required>
		</div>
		<button type="submit">Modifier</button>
	</form>
</div>
